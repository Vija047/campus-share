{
  "version": 2,
  "name": "campus-share-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "uploads/**",
          "utils/**",
          "models/**",
          "routes/**",
          "controllers/**",
          "middleware/**",
          "socket/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/notes/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/posts/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/chat/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/stats/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": [
    "iad1"
  ]
}