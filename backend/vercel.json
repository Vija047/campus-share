{
  "version": 2,
  "name": "campus-share-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node@3"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/notes/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/posts/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/chat/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/chatbot/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/notifications/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/stats/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/health",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}